local placeIDAtual = game.PlaceId
local placeIDDesejado = 5956785391

if placeIDAtual == placeIDDesejado then
    local args = {
        [1] = "join",
        [2] = PrivateServerCode,
        [3] = 6152116144
    }
    
    local replicatedStorage = game:GetService("ReplicatedStorage")
    local handlePrivateServer = replicatedStorage:WaitForChild("handle_privateserver")

    handlePrivateServer:InvokeServer(unpack(args))
else
    print("Você não está no local correto para realizar o teleporte.")
end

wait(0.1)

local PlaceIDToExecute = 6152116144
local TeleportPlaceID = 5956785391
local CurrentPlaceID = game.PlaceId
local TimerDuration = 0 * 5  -- 30 minutos em segundos

if CurrentPlaceID == PlaceIDToExecute then
    print("Executando o script no Place ID correto.")

    -- Esperar 30 minutos
    print("Aguardando 20 minutos...")
    wait(TimerDuration)

    -- Verificar se o Place ID ainda é o correto antes de teleportar
    if game.PlaceId == PlaceIDToExecute then
        -- Teleportar para o Place ID desejado
        game:GetService("TeleportService"):Teleport(TeleportPlaceID)
        print("Teleportado para o Place ID", TeleportPlaceID)
    else
        print("O Place ID mudou. Não foi possível teleportar.")
    end
else
    print("Este script só deve ser executado no Place ID", PlaceIDToExecute)
    print("Encerrando a execução do script.")
end
